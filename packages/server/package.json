{
  "name": "@graphand/server",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    }
  },
  "types": "./src/index.ts",
  "main": "./src/index.ts",
  "scripts": {
    "lint": "npm run eslint",
    "eslint": "eslint . --max-warnings 0",
    "dev": "bun --watch --env-file=.env src/entries/run.ts",
    "start": "bun --env-file=.env src/entries/run.ts"
  },
  "dependencies": {
    "@graphand/core": "*",
    "@graphand/server-module-example": "*",
    "@graphand/server-module-database": "*"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "bun-types": "^1.2.13",
    "@repo/eslint-config": "*",
    "@repo/typescript-config": "*"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}
