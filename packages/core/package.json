{
  "name": "@graphand/core",
  "version": "1.1.4",
  "private": false,
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    }
  },
  "types": "./dist/index.d.ts",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "scripts": {
    "lint": "eslint . --max-warnings 0",
    "build": "vite build",
    "build:watch": "vite build --watch",
    "dev": "vite",
    "build:types": "tsc --emitDeclarationOnly",
    "test": "vitest run"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@repo/eslint-config": "*",
    "@repo/typescript-config": "*",
    "@types/eslint": "^8.56.5",
    "@types/node": "^20.14.8",
    "bson": "^6.8.0",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "prettier": "^2.8.0",
    "ts-loader": "^9.4.2",
    "ts-patch": "^3.1.2",
    "typescript": "^5.3.3",
    "vite": "^5.3.2",
    "vite-plugin-dts": "^3.9.1",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^2.1.1"
  },
  "files": [
    "dist",
    "README.md",
    "!**/*.spec.d.ts",
    "!**/*.dev.d.ts",
    "!**/*.map"
  ]
}
