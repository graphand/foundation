{
  "name": "@graphand/cli",
  "version": "1.2.11",
  "private": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js"
    }
  },
  "types": "./dist/index.d.ts",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.es.js",
  "bin": {
    "graphand": "dist/bin.es.js"
  },
  "scripts": {
    "lint": "eslint . --max-warnings 0",
    "build": "vite build",
    "build:watch": "vite build --watch",
    "dev": "vite build --watch",
    "build:types": "tsc --emitDeclarationOnly",
    "test": "vitest run"
  },
  "devDependencies": {
    "@repo/eslint-config": "*",
    "@repo/typescript-config": "*",
    "@types/archiver": "^6.0.3",
    "@types/qs": "^6.9.15",
    "builtin-modules": "^4.0.0"
  },
  "dependencies": {
    "@graphand/client": "*",
    "@graphand/client-module-auth": "^1.3.2",
    "@inquirer/prompts": "^5.3.6",
    "archiver": "^7.0.1",
    "chalk": "^5.3.0",
    "cli-table3": "^0.6.5",
    "commander": "^12.1.0",
    "conf": "^13.0.1",
    "esbuild": "^0.23.0",
    "mime": "^4.0.4",
    "open": "^10.1.0",
    "ora": "^8.0.1",
    "qs": "^6.13.0"
  },
  "files": [
    "dist",
    "README.md",
    "!**/*.spec.d.ts",
    "!**/*.dev.d.ts",
    "!**/*.spec.d.ts",
    "!**/*.map"
  ]
}
